

<%= render 'shared/errors', 
    obj: @category,  
    msg: "You cannot create the category:" %>

<%= form_for @category do |f| %>
  <div class="form-group">
    <%= f.label :name, "Category Name" %>
    <%= f.text_field :name %>
  </div>
    <%= f.submit "Create category", class: "btn btn-default" %>
<% end %>
